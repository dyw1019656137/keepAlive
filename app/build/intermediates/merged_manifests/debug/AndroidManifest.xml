    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <service
            android:name="com.dyw.keepalive.service.MusicService"
            android:priority="1000"
            ndroid:persistent="true" />
        <service
            android:name="com.dyw.keepalive.service.CoreService"
            android:persistent="true" />
        <service
            android:name="com.dyw.keepalive.service.GpsService"
            android:persistent="true"
            android:priority="1000" />

        <activity android:name="com.dyw.keepalive.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>